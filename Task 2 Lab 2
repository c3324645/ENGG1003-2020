#include <stdio.h>

int main()
{
    float x1, x2, x3, x4;
    float mean, variance;
    
    scanf("%f %f %f %f", &x1, &x2, &x3, &x4);
    mean=(x1+x2+x3+x4)*1/4;
    variance=((x1-mean)*(x1-mean)+(x2-mean)*(x2-mean)+(x3-mean)*(x3-mean)+(x4-mean)*(x4-mean))*1/3;
    printf("Variace is: %f\n", variance);

return 0;
}
