#include <stdio.h>
#include <stdlib.h>

int main()
{
   float Celcius, Farenheit;

    printf("Enter the temperature in Farenheit: ");

    scanf("%f", &Farenheit);

    Celcius=(Farenheit-32)*5/9; // 5/9 works if it is placed last but will work at the start of the eqn if (float)5/9 is used or 5.0/9.0 is used

    printf("The temperature in Celcius is: %.2f", Celcius, Farenheit);

    return 0;
}
