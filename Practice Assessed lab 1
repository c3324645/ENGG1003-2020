// Practice Assessed lab 1 - Pass/credit level

int main()
{
    float x0=0.65;
    float xn;
    float xnml=0.65;
    int n=1;

    printf("%d %.6f\n", n, x0);

    xn=x0;

    while(n<10 || xn-xnml<0.001)
    {
        xn=(0.2*xnml)+5;

        printf("%d %.6f\n", n, xn);
        xnml=xn;
        xn++;
        n++;

    }

    return 0;
}
