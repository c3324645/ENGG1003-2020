#include <stdio.h>

int main()
{
    float x1, y1, x2, y2, x, y;
    
    printf("Enter point 1: ");
    scanf("%f %f", &x1, &y1);
    printf("Enter point 2: ");
    scanf("%f %f", &x2, &y2);
    scanf("%f", &x);
    
    y=((y2-y1)/(x2-x1))*(x-x1)+y1;
    printf("The value of y is: %f", y);

return 0;
}
